function fact(n) {
    var acc = 1;
    while (n > 0) {
        acc = acc * n;
        n = n - 1;
    }
    return acc;
}

function isodd(n) {
    var diff = 2;
    if (n < 0)
        diff = -2;
    var loopcond = 1;
    while (loopcond) {
        if (n == 0)
            loopcond = 0;
        if (n == 1)
            loopcond = 0;
        if (loopcond)
            n = n - diff;
    }
    return n;
}

var f = fact(10);
var o = isodd(f);

if (o) {
    print(123);
} else {
    print(234);
}
